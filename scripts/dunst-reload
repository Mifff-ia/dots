#!/bin/sh
#
# Script to set colors generated by 'wal'
# https://github.com/dylanaraps/wal
# Source generated colors.
. "${HOME}/.cache/wal/colors.sh"

pkill dunst
dunst \
    -lb   "${color0}" \
    -nb   "${color0}" \
    -cb   "${color0}" \
    -lf   "${color7}" \
    -cf   "${color7}" \
    -nf   "${color7}" \
    -lfr  "${color4}" \
    -nfr  "${color7}" \
    -cfr  "${color1}" \
    -font "Iosevka Fixed" &
